{
  "name": "vue3-template",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "A Vue 3 project template with TypeScript, TSX, Vite, Vue Router, Pinia, Naive UI, Nice-Axios, and more.",
  "packageManager": "pnpm@9.0.1",
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build --mode production && esno ./build/scripts/postBuild.ts production",
    "build:staging": "cross-env NODE_ENV=staging vite build --mode staging && esno ./build/scripts/postBuild.ts staging",
    "swagger": "esno ./build/scripts/swagger/index.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.8",
    "naive-ui": "^2.38.1",
    "nice-axios": "^0.1.19",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.23",
    "vue-i18n": "^9.13.0",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.31",
    "@intlify/unplugin-vue-i18n": "^0.9.3",
    "@types/markdown-it-link-attributes": "^3.0.5",
    "@types/nprogress": "^0.2.3",
    "@types/shelljs": "^0.8.15",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/test-utils": "^2.4.5",
    "@yc-tech/eslint-config": "^0.1.1",
    "@yc-tech/prettier-config": "^0.1.0",
    "autoprefixer": "^10.4.19",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^12.17.4",
    "dotenv": "^16.4.5",
    "esno": "^4.7.0",
    "https-localhost": "^4.7.1",
    "less": "^4.2.0",
    "pnpm": "^7.33.7",
    "rollup-plugin-visualizer": "^5.12.0",
    "shelljs": "^0.8.5",
    "shiki": "^0.14.7",
    "simple-git-hooks": "^2.11.1",
    "swagger-typescript-api": "^12.0.4",
    "tailwindcss": "^3.4.3",
    "taze": "^0.13.6",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^5.2.9",
    "vite-plugin-externals": "^0.6.2",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-inspect": "^0.8.3",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "^0.29.8",
    "vue-tsc": "^1.8.27"
  }
}
